<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="759.0" prefWidth="1075.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.maintermprojectjavafx.SelectEquipmentAppl">
   <children>
      <Label alignment="CENTER" prefHeight="91.0" prefWidth="1075.0" style="-fx-background-color: Lavender;" text="Select equipment.Remember,you can only choose one equipment of each type">
         <font>
            <Font name="Times New Roman" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="EquipTable" layoutX="32.0" layoutY="124.0" prefHeight="294.0" prefWidth="1011.0">
         <columns>
            <TableColumn fx:id="nameEquip" prefWidth="208.0" text="Name" />
            <TableColumn fx:id="materialEquip" prefWidth="184.0" text="Material" />
            <TableColumn fx:id="costEquip" prefWidth="136.0" text="Cost" />
            <TableColumn fx:id="weightEquip" prefWidth="151.0" text="Weight" />
            <TableColumn fx:id="strengthEquip" prefWidth="142.0" text="Strength" />
            <TableColumn fx:id="typeofarmorEquip" prefWidth="188.0" text="Type" />
         </columns>
      </TableView>
      <Button layoutX="32.0" layoutY="673.0" mnemonicParsing="false" onAction="#BackToSelectKnight" prefHeight="60.0" prefWidth="107.0" text="Back">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="267.0" layoutY="562.0" mnemonicParsing="false" onAction="#buttonGetDataEquipment" prefHeight="60.0" prefWidth="215.0" text="Get Data">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Button>
      <Button layoutX="579.0" layoutY="562.0" mnemonicParsing="false" onAction="#chooseEquipment" prefHeight="60.0" prefWidth="215.0" text="Choose equipment">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
